{
  "cli": {
    "version": ">= 7.6.0"
  },
  "build": {
    "local": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "local",
        "EXPO_PUBLIC_KOFI_URL": "https://ko-fi.com/gregamann",
        "EXPO_PUBLIC_CONTACT_EMAIL_URL": "mailto:gregoire.amann@gmail.com",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://expo.dev",
        "EXPO_PUBLIC_TERMS_OF_USE_URL": "https://expo.dev"
      },
      "channel": "local"
    },
    "local-simulator": {
      "channel": "local-simulator",
      "extends": "local",
      "ios": {
        "simulator": true
      },
      "env": {}
    },
    "preview": {
      "channel": "preview",
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_KOFI_URL": "https://ko-fi.com/gregamann",
        "EXPO_PUBLIC_CONTACT_EMAIL_URL": "mailto:gregoire.amann@gmail.com",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://expo.dev",
        "EXPO_PUBLIC_TERMS_OF_USE_URL": "https://expo.dev"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_KOFI_URL": "https://ko-fi.com/gregamann",
        "EXPO_PUBLIC_CONTACT_EMAIL_URL": "mailto:gregoire.amann@gmail.com",
        "EXPO_PUBLIC_PRIVACY_POLICY_URL": "https://expo.dev",
        "EXPO_PUBLIC_TERMS_OF_USE_URL": "https://expo.dev"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
